{
  "name": "ChimeraSociety-Code",
 "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

     "ReplicatedFirst": {
      "ClientSetup":{
        "$path": "src/ReplicatedFirst/ClientSetup" 
      }
    },

    "ReplicatedStorage": {
      "Libraries": {
        "$path": "src/ReplicatedStorage/Libraries"
      },
      "Remotes": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,
        "$path": "src/ReplicatedStorage/Remotes"
      },
      "SharedModules": {
        "$path": "src/ReplicatedStorage/SharedModules"
      },

      "SharedAssets":{
        "$className": "Folder",

        "Characters":{
          "$className": "Folder",
          "$ignoreUnknownInstances": true,
          "$path": "src/ReplicatedStorage/SharedAssets/Characters"
        },
        "NPCs":{
          "$className": "Folder",
          "$ignoreUnknownInstances": true,
          "$path": "src/ReplicatedStorage/SharedAssets/NPCs"
        }
      }
    },

    "ServerScriptService": {
      "AI": {
        "$className": "Actor",
        "$ignoreUnknownInstances": true,
        "$path": "src/ServerScriptService/AI"
      },
      "HumanoidManager":{
        "$ignoreUnknownInstances": true,
        "$path": "src/ServerScriptService/HumanoidManager"
      },
      "RunServer":{
        "$ignoreUnknownInstances": true,
        "$path": "src/ServerScriptService/RunServer"
      }
    },

    "StarterGui": {
      "OnscreenInfo": {
        "$className": "ScreenGui",
        "$ignoreUnknownInstances": true,
        "$path": "src/StarterGui/OnscreenInfo"
      }
    },

    "StarterPlayer": {
      "StarterCharacterScripts":{
        "$className": "StarterCharacterScripts",
        "$ignoreUnknownInstances": true,
        "$path": "src/StarterCharacterScripts"
      },
      
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$ignoreUnknownInstances": true,
        "$path": "src/StarterPlayerScripts"
      }
    }
  }
}